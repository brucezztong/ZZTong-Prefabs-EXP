<lootcontainers>

    <!-- Add the duck from the J. Warren McClure School of Emerging Communication Technologies. -->
    <append xpath="/lootcontainers/lootgroup[@name='groupFileCabinet01']">
        <item name="zztongRubberDucky" count="1"/>
    </append>


    <!-- Fred E. Fromage Boxes are a blend of Shamway and Mo-Power shelf loot. -->
	<insertAfter xpath="/lootcontainers/lootcontainer[@id='551']">
        <lootcontainer id="1000" name="zztongStorageBoxFredEFromage" count="1" size="8,9" sound_open="UseActions/open_chest" sound_close="UseActions/close_chest" loot_quality_template="qualBaseTemplate">
            <item name="foodCanSham" count="1,3"/>
            <item group="groupFoodTrash" loot_prob_template="high"/>
            <item group="groupFoodCommon" loot_prob_template="low"/>
            <item group="groupDrinksCommon" loot_prob_template="low"/>
            <item name="resourceScrapPolymers" count="2,4" loot_prob_template="med"/>
            <item name="resourceScrapIron" count="2,10" loot_prob_template="med"/>
            <item name="resourcePaper" count="2,4" loot_prob_template="med"/>
            <item name="resourceHeadlight" count="1" loot_prob_template="med"/>
            <item name="resourceElectricParts" count="1,2" loot_prob_template="medLow"/>
            <item name="resourceMechanicalParts" count="1,2" loot_prob_template="medLow"/>
            <item group="groupElectricalDevices" loot_prob_template="veryLow"/>
        </lootcontainer>
	</insertAfter>

</lootcontainers>
